{
  "name": "remix-template-vercel",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "vercel": "tailwindcss -m -i app/styles/tailwind.css -o app/styles/tailwind-out.css && remix dev",
    "build": "run-p build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:css": "npm run generate:css -- --watch",
    "generate:css": "tailwindcss -m -i app/styles/tailwind.css -o app/styles/tailwind-out.css"
  },
  "dependencies": {
    "@remix-run/node": "^1.4.1",
    "@remix-run/react": "^1.4.1",
    "@remix-run/vercel": "^1.4.1",
    "@vercel/node": "^1.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.1",
    "@remix-run/eslint-config": "^1.4.1",
    "@remix-run/serve": "^1.4.1",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24"
  },
  "engines": {
    "node": ">=14"
  }
}
